    "template",
    "starter",
    "boilerplate",
    "infrastructure",
    "production-ready"
  ],
  "license": "MIT",
  "author": "йцу",
  "repository": {
    "type": "git",
    "url": "https://github.com/kinderlystv-png/project-template.git"
  },
  "homepage": "https://github.com/kinderlystv-png/project-template#readme",
  "bugs": {
    "url": "https://github.com/kinderlystv-png/project-template/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "type-check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run --config=tests/vitest.config.ts",
    "test:ui": "vitest --ui --config=tests/vitest.config.ts",
    "test:run": "vitest run --config=tests/vitest.config.ts",
    "test:watch": "vitest --config=tests/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config=tests/vitest.config.ts",
    "test:unit": "vitest run tests/unit --config=tests/vitest.config.ts",
    "test:integration": "vitest run tests/integration --config=tests/vitest.config.ts",
    "test:performance": "vitest run tests/performance --config=tests/vitest.config.ts",
    "test:visual": "vitest run tests/visual --config=tests/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:all": "pwsh ./scripts/run-all-tests.ps1",
    "test:quick": "pwsh ./scripts/test-runner.ps1",
    "test:help": "pwsh ./scripts/test-runner.ps1 --help",
    "template:update": "node scripts/update-template.js",
    "template:test": "npm install && npm run test && npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@types/three": "^0.157.0",
    "autoprefixer": "^10.4.16",
    "framer-motion": "^10.16.4",
    "gsap": "^3.12.2",
    "lottie-web": "^5.12.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.6",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^4.2.3",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^20.19.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.46.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "terser": "^5.44.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
